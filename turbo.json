{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".medusa/**", "public/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"generate:oas": {
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"format": {
			"cache": false
		}
	}
}
