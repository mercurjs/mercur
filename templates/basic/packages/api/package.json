{
  "name": "@acme/api",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "exports": {
    "./_generated": {
      "types": "./.mercur/_generated/index.ts"
    }
  },
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "mercurjs codegen && medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev:medusa": "medusa develop",
    "dev": "concurrently --kill-others npm:dev:*",
    "dev:codegen": "mercurjs codegen --watch",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.13.0",
    "@medusajs/cli": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/dashboard": "^2.13.0",
    "@medusajs/admin-shared": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/draft-order": "2.13.0",
    "@mercurjs/core-plugin": "2.0.0-canary.4",
    "@mercurjs/types": "2.0.0-canary.4",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.13.0",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "concurrently": "^9.2.1",
    "typescript": "5.9.3",
    "vite": "^5.4.21",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}