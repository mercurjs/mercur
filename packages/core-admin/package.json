{
  "name": "@mercurjs/core-admin",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "generate:static": "node ./scripts/generate-currencies.js && prettier --write ./src/lib/currencies.ts",
    "dev": "vite",
    "build": "tsup && node ./scripts/generate-types.js",
    "build:preview": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "main": "dist/app.js",
  "module": "dist/app.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.js",
      "types": "./dist/index.d.ts"
    },
    "./css": {
      "import": "./dist/app.css",
      "require": "./dist/app.css"
    },
    "./src/*": "./src/*",
    "./pages/*": "./src/pages/*/index.tsx",
    "./components": "./src/components/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./providers": "./src/providers/index.ts",
    "./lib": "./src/lib/index.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
  "dependencies": {
    "@ariakit/react": "^0.4.15",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "3.4.2",
    "@medusajs/admin-shared": "2.10.3",
    "@medusajs/icons": "2.10.3",
    "@medusajs/js-sdk": "2.10.3",
    "@medusajs/ui": "4.0.23",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dismissable-layer": "1.1.4",
    "@talkjs/react": "^0.1.11",
    "@tanstack/react-query": "5.64.2",
    "@tanstack/react-table": "8.20.5",
    "@tanstack/react-virtual": "^3.8.3",
    "@uiw/react-json-view": "^2.0.0-alpha.17",
    "cmdk": "^0.2.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "i18next": "23.7.11",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.2",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.4",
    "motion": "^11.15.0",
    "qs": "^6.12.0",
    "radix-ui": "1.1.2",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-currency-input-field": "^3.6.11",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "7.49.1",
    "react-i18next": "13.5.0",
    "react-jwt": "^1.2.0",
    "react-router-dom": "6.20.1",
    "talkjs": "^0.38.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@medusajs/admin-shared": "2.10.3",
    "@medusajs/admin-vite-plugin": "2.10.3",
    "@mercurjs/vite-plugin": "^0.0.1",
    "@medusajs/types": "2.10.3",
    "@medusajs/ui-preset": "2.10.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.11.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/stripe": "^8.0.416",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "4.2.1",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-classnames": "^0.8.4",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "5.2.2",
    "vite": "^5.4.14",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^3.0.5"
  }
}
