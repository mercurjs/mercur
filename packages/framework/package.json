{
  "name": "@mercurjs/framework",
  "author": "MercurJS <hello@mercurjs.com> (https://mercurjs.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/mercurjs/mercur"
  },
  "keywords": [
    "mercur",
    "marketplace",
    "medusajs",
    "mercurjs"
  ],
  "version": "1.5.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/__tests__",
    "!dist/**/__mocks__",
    "!dist/**/__fixtures__"
  ],
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc --build",
    "migration:initial": " MIKRO_ORM_CLI_CONFIG=./mikro-orm.config.dev.ts medusa-mikro-orm migration:create --initial",
    "migration:create": " MIKRO_ORM_CLI_CONFIG=./mikro-orm.config.dev.ts medusa-mikro-orm migration:create",
    "migration:up": " MIKRO_ORM_CLI_CONFIG=./mikro-orm.config.dev.ts medusa-mikro-orm migration:up",
    "orm:cache:clear": " MIKRO_ORM_CLI_CONFIG=./mikro-orm.config.dev.ts medusa-mikro-orm cache:clear"
  },
  "devDependencies": {
    "@medusajs/index": "2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/test-utils": "2.11.3",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "@medusajs/framework": "2.11.3"
  }
}
