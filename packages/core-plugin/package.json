{
  "name": "@mercurjs/core-plugin",
  "version": "2.0.0-canary.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/mercurjs/mercur",
    "directory": "packages/core-plugin"
  },
  "description": "Core plugin for MercurJS.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./_generated": {
      "types": "./.mercur/_generated/index.ts"
    },
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": {
      "types": "./.medusa/server/src/modules/*/index.d.ts",
      "default": "./.medusa/server/src/modules/*/index.js"
    },
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./*": "./.medusa/server/src/*.js"
  },
  "keywords": [
    "medusa",
    "plugin",
    "medusa-plugin-other",
    "medusa-plugin",
    "medusa-v2"
  ],
  "scripts": {
    "build": "tsc --declaration --outDir .medusa/server",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "tsc --declaration --outDir .medusa/server"
  },
  "devDependencies": {
    "cors": "^2.8.5",
    "@medusajs/core-flows": "^2.13.0",
    "@medusajs/cli": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/icons": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/test-utils": "^2.13.0",
    "@medusajs/ui": "4.1.1",
    "@mercurjs/types": "workspace:*",
    "@swc/core": "^1.7.28",
    "@types/cors": "^2.8.17",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.9.3",
    "yalc": "^1.0.0-pre.53",
    "@medusajs/types": "2.13.0",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.13.0",
    "@medusajs/cli": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/icons": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/test-utils": "^2.13.0"
  },
  "engines": {
    "node": ">=20"
  }
}