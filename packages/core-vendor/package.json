    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@medusajs/admin-shared": "^2.5.0",
    "@medusajs/admin-vite-plugin": "^2.5.0",
    "@medusajs/types": "^2.5.0",
    "@medusajs/ui-preset": "^2.5.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.11.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
