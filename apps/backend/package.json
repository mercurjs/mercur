{
  "name": "api",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && rm -f public && ln -s .medusa/server/public/ public",
    "build:admin": "medusa build --only-admin",
    "build:windows": "medusa build && node -e \"const fs=require('fs');const path=require('path');const target='.medusa/server/public/';const link='public';try{if(fs.existsSync(link))fs.rmSync(link,{recursive:true,force:true});fs.symlinkSync(target,link,process.platform==='win32'?'junction':'dir');console.log('✔ Symlink created:',link,'→',target);}catch(e){console.error('✖ Failed to create symlink:',e.message);process.exit(1);}\"",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start --types=false",
    "dev": "medusa develop --types=false",
    "db:migrate": "medusa db:migrate",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "format": "prettier --write .",
    "lint": "eslint ./src ",
    "lint:fix": "eslint ./src --fix ",
    "generate:oas": "medusa-oas oas --type combined --paths ./src/api"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.2",
    "@medusajs/cli": "2.10.2",
    "@medusajs/framework": "2.10.2",
    "@medusajs/medusa": "2.10.2",
    "@mercurjs/algolia": "*",
    "@mercurjs/b2c-core": "npm:mercurjs-b2c-core@1.5.2",
    "@mercurjs/commission": "*",
    "@mercurjs/framework": "npm:mercurjs-framework@1.4.4",
    "mercurjs-payment-adyen-connect": "*",
    "@mercurjs/payment-stripe-connect": "*",
    "@mercurjs/requests": "*",
    "@mercurjs/resend": "*",
    "@mercurjs/reviews": "*",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@talkjs/react": "^0.1.11",
    "algoliasearch": "^5.20.2",
    "awilix": "^8.0.1",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.0",
    "resend": "^4.1.2",
    "stripe": "^17.4.0",
    "talkjs": "^0.38.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@medusajs/medusa-oas-cli": "2.10.2",
    "@medusajs/test-utils": "2.10.2",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.2.11"
  },
  "engines": {
    "node": ">=20"
  }
}
